{
	"name": "arr2504/vuez",
	"license": "MIT",
	"type": "library",
	"description": "Vuez provides a simple starting point for building a SPA (single-page application) using Vue or React with the Laravel framework. The code structure remains unchanged, and Vuez can be styled using either Bootstrap or Tailwind.",
	"keywords": [
		"laravel",
		"auth"
	],
	"support": {
		"issues": "https://github.com/arr2504/vuez/issues",
		"source": "https://github.com/arr2504/vuez"
	},
	"authors": [
		{
			"name": "ramddan",
			"email": "ramddan@icloud.com"
		}
	],
	"require": {
		"php": "^8.1.0",
		"laravel/sanctum": "^3.2",
		"illuminate/console": "^10.0",
		"illuminate/filesystem": "^10.0",
		"illuminate/support": "^10.0",
		"illuminate/validation": "^10.0"
	},
	"autoload": {
		"psr-4": {
			"Arr2504\\Vuez\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"laravel": {
			"providers": [
				"Arr2504\\Vuez\\VuezServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
